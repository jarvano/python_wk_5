# Base class
class Vehicle:
    def move(self):
        pass  # Method to be overridden by subclasses


# Subclasses with different implementations of move()
class Car(Vehicle):
    def move(self):
        print("ğŸš— Driving on the road...")


class Plane(Vehicle):
    def move(self):
        print("âœˆï¸ Flying in the sky...")


class Boat(Vehicle):
    def move(self):
        print("â›µ Sailing on the water...")


class Bicycle(Vehicle):
    def move(self):
        print("ğŸš´ Pedaling on the path...")


# ---- Example usage ----
vehicles = [Car(), Plane(), Boat(), Bicycle()]

for v in vehicles:
    v.move()   # Same method name, different behavior
